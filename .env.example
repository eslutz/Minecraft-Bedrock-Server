# Minecraft Bedrock Server - Environment Variables Template
# Copy this file to .env and fill in your values

# Container Settings (Host-Specific)
# Run 'id' command to get your UID and GID
UID=1000
GID=1000

# Security & Access Control
ALLOW_LIST_USERS=gamertag1:xuid1,gamertag2:xuid2
OPS=xuid1

# Server & World Identity
SERVER_NAME=My Minecraft Server
LEVEL_NAME=My World Name
LEVEL_SEED=
